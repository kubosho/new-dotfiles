{
  "autoUpdatesChannel": "latest",
  "statusLine": {
    "type": "command",
    "command": "bunx -y ccstatusline@latest",
    "padding": 0
  },
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    }
  },
  "permissions": {
    "deny": [
      "NotebookEdit"
    ]
  }
}
